<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension
	    point="org.eclipse.ui.actionSets">
	    <actionSet
            description="Analyzes a pointcut expression."
            id="uk.ac.lancs.comp.khatchad.analyze.pointcut"
            label="Rejuvinate Pointcut"
            visible="true">
         <action
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.AnalyzePointcutPlugin"
               definitionId="uk.ac.lancs.comp.khatchad.analyze.pointcut"
               enablesFor="+"
               helpContextId="uk.ac.lancs.comp.khatchad.AnalyzePointcut.help"
               id="uk.ac.lancs.comp.khatchad.AnalyzePointcut.action"
               label="&amp;Analyze Pointcut..."
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/codingGroup2"
               style="push"
	           toolbarPath="Normal"
               tooltip="Analyze Pointcut"/>
         <action
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.RejuvenatePointcutPlugin"
               definitionId="uk.ac.lancs.comp.khatchad.rejuvenate.pointcut"
               helpContextId="uk.ac.lancs.comp.khatchad.RejuvenatePointcut.help"
               id="uk.ac.lancs.comp.khatchad.RejuvinatePointcut.action"
               label="&amp;Rejuvenate Pointcut..."
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/codingGroup2"
               style="push"
               toolbarPath="Normal"
               tooltip="Rejuvenate Pointcut"/>
      </actionSet>
   </extension>
      <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.ajdt.core.javaelements.AdviceElement"
            adaptable="true"
            id="uk.ac.lancs.comp.khatchad.rejuvenate.pointcut.object.contribution">
         <action
               label="Analyze Pointcut..."
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.AnalyzePointcutPlugin"
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/reorgGroup2"
               enablesFor="+"
               id="uk.ac.lancs.comp.khatchad.analyze.pointcut.popup">
         </action>
         <action
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.RejuvenatePointcutPlugin"
               id="uk.ac.lancs.comp.khatchad.rejuvenate.pointcut.popup"
               label="Rejuvenate Pointcut..."
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/reorgGroup2">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="uk.ac.lancs.comp.khatchad.analyze.pointcut.project.contribution">
         <action
               label="Analyze Pointcut..."
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.AnalyzePointcutPlugin"
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/reorgGroup2"
               enablesFor="+"
               id="uk.ac.lancs.comp.khatchad.analyze.pointcut.project.popup">
         </action>
      </objectContribution>
   </extension>
      <extension
            point="org.eclipse.ui.views">
         <category
               id="uk.ac.lancs.comp.khatchad.rejuvenatepc.ui"
               name="Rejuvenate Pointcut">
         </category>
         <view
               category="uk.ac.lancs.comp.khatchad.rejuvenatepc.ui"
               class="uk.ac.lancs.comp.khatchad.rejuvenatepc.ui.views.SuggestionView"
               icon="icons/sample.gif"
               id="uk.ac.lancs.comp.khatchad.rejuvenatepc.ui.views.SuggestionView"
               name="Suggestions">
         </view>
      </extension>
      <extension
            point="org.eclipse.ui.perspectiveExtensions">
         <perspectiveExtension
               targetID="org.eclipse.ui.resourcePerspective">
            <view
                  id="uk.ac.lancs.comp.khatchad.rejuvenatepc.ui.views.SuggestionView"
                  ratio="0.5"
                  relationship="right"
                  relative="org.eclipse.ui.views.TaskList">
            </view>
         </perspectiveExtension>
      </extension>
